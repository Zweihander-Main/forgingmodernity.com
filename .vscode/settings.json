{
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.formatOnSave": true,
	"files.exclude": {
		"**/node_modules": true
	},
	"vim.useSystemClipboard": true,
	"cSpell.enableFiletypes": ["astro"],
	"[astro]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[svelte]": {
		"editor.defaultFormatter": "svelte.svelte-vscode"
	},
	"svelte.plugin.svelte.diagnostics.enable": false,
	"eslint.probe": [
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact",
		"html",
		"vue",
		"markdown",
		"astro",
		"svelte"
	],
	"eslint.validate": [
		"astro",
		"svelte",
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact",
	],
	"stylelint.validate": [
		"css",
		"less",
		"postcss",
		"scss",
		"html",
		"svelte",
		"astro"
	],
	"cSpell.words": [
		"Aire",
		"Atherstone",
		"Austell",
		"authorised",
		"backface",
		"bookoverview",
		"Boulton",
		"Brindley",
		"Burslem",
		"Cherwell",
		"Egerton",
		"Fazeley",
		"fetchpriority",
		"Foulridge",
		"Fradley",
		"funcs",
		"Gargrave",
		"Harecastle",
		"Hawkesbury",
		"Kidsgrove",
		"lucide",
		"lvml",
		"Middlesex",
		"Newdigate",
		"noninteractive",
		"Skipton",
		"Smethwick",
		"srcset",
		"Stourport",
		"Tatton",
		"tselint",
		"undersubscribed",
		"webp",
		"Wednesbury",
		"zoomstart"
	]
}
